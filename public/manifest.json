{
  "name": "Iconfonter",
  "description": "iconfont.cn 辅助工具。",
  "version": "1.3.0",
  "manifest_version": 2,
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "http://at.alicdn.com/",
    "https://at.alicdn.com/",
    "http://*.iconfont.cn/",
    "https://*.iconfont.cn/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.iconfont.cn/manage/index*"
      ],
      "js": [
        "assets/Common.js",
        "assets/ManageProject.js"
      ],
      "css": [
        "assets/Common.css",
        "assets/ManageProject.css"
      ]
    },
    {
      "matches": [
        "*://*.iconfont.cn/*"
      ],
      "js": [
        "assets/Common.js",
        "assets/CollectionDetail.js"
      ],
      "css": [
        "assets/Common.css",
        "assets/CollectionDetail.css"
      ]
    }
  ]
}
